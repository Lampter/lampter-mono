{
    "name": "@lampter/server",
    "version": "1.0.0",
    "main": "cjs/index.js",
    "types": "cjs/index.d.ts",
    "scripts": {
        "clean": "rimraf ./cjs",
        "prebuild": "yarn clean",
        "build": "tsc -p tsconfig.build.json",
        "test": "mocha \"test/**/*.spec.ts?(x)\"",
        "prestart": "yarn workspace @lampter/client build:umd",
        "start": "nodemon -r @ts-tools/node/r -r tsconfig-paths/register --ext ts,tsx,js,json ./src/index.tsx",
        "prepack": "yarn build"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "dependencies": {
        "@lampter/client": "^1.0.0",
        "compression": "^1.7.4",
        "express": "^4.17.1"
    },
    "files": [
        "cjs",
        "src"
    ],
    "license": "MIT",
    "repository": "git@github.com:wixplosives/lampter.git",
    "private": true,
    "publishConfig": {
        "access": "public"
    }
}
