    "prepack": "yarn build"
  },
  "peerDependencies": {},
  "dependencies": {
    "apollo-server-express": "^2.9.7",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-json": "^1.0.0",
    "express-jwt": "^5.3.1",
    "graphql": "^14.5.8",
    "graphql-type-json": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0",
    "sqlite3": "^4.1.0",
    "type-graphql": "^0.17.5"
  },
  "files": [
    "cjs",
    "umd",
    "src"
