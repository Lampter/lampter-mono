{
  "name": "@lampter/server",
  "version": "1.0.0",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs",
    "prebuild": "yarn clean",
    "build": "tsc -p tsconfig.build.json",
    "test": "mocha \"test/**/*.spec.ts?(x)\"",
    "watch-node": "nodemon cjs/server.js",
    "watch-ts": "tsc -p tsconfig.build.json -w",
